<odoo>

    <record id="fsm_stage_form_view_inherit_tracking" model="ir.ui.view">
        <field name="name">fsm.stage.form.inherit.tracking</field>
        <field name="model">fsm.stage</field>
        <field name="inherit_id" ref="fieldservice.fsm_stage_form_view" />
        <field name="arch" type="xml">
            <field name="is_default" position="after">
                <field name="start_tracking" />
                <field name="stop_tracking" />
                <field name="notify_customer" />
                <field name="notify_dispatcher" />
                <field name="distance_to_next_stop" />
            </field>
        </field>
    </record>

</odoo>
