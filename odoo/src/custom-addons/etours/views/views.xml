<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="etours.list">
      <field name="name">etours list</field>
      <field name="model">etours.etours</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="etours.action_window">
      <field name="name">etours window</field>
      <field name="res_model">etours.etours</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->

    <record model="ir.actions.server" id="etours.action_server">
      <field name="name">etours server</field>
      <field name="model_id" ref="model_etours_etours"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="etours" id="etours.menu_root"/>
    <!-- menu categories -->

    <menuitem name="Menu 1" id="etours.menu_1" parent="etours.menu_root"/>
    <menuitem name="Menu 2" id="etours.menu_2" parent="etours.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="etours.menu_1_list" parent="etours.menu_1"
              action="etours.action_window"/>
    <menuitem name="Server to list" id="etours" parent="etours.menu_2"
              action="etours.action_server"/>

  </data>
</odoo>
