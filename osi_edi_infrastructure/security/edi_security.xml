<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
	
		<!-- EDI Provider -->
		<record model="ir.module.category" id="edi_provider_module">
		  <field name="name">EDI Provider</field>
		  <field name="description">User access level for EDI Provider</field>
		  <field name="sequence">3</field>
		</record>
	
		<record id="group_edi_provider_user" model="res.groups">
			<field name="name">EDI Provider User</field>
			<field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
			<field name="users" eval="[(4, ref('base.user_root'))]"/>
			<field name="category_id" ref="osi_edi_infrastructure.edi_provider_module"/>
		</record>
	
	    <record id="group_edi_provider_manager" model="res.groups">
	      <field name="name">EDI Provider Manager</field>
	      <field name="implied_ids" eval="[(4, ref('osi_edi_infrastructure.group_edi_provider_user'))]"/>
	      <field name="category_id" ref="osi_edi_infrastructure.edi_provider_module"/>
	    </record>

	</data>
</odoo>
