    <!-- Agreement Form View -->
	<record id="agreement_subscription_form_view" model="ir.ui.view">
        <field name="name">agreement.subscription.form.view</field>
        <field name="model">agreement</field>
        <field name="inherit_id" ref="agreement.partner_agreement_form_view"/>
        <field name="arch" type="xml">
            <group name="sale_group" position="after">
                <group>
                    <field name="subscription_id" readonly="1"/>
                </group>
            </group>
        </field>
    </record>
</odoo>